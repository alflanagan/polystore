{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
	"jasmine": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "double"],
        "semi": ["error", "never"],
	"eqeqeq": ["error", "always"],
	"no-useless-call": ["error"],
	"no-console": "off"
    }
}
